public class MatematikaBeraksi {
    public static void main(String[] args) {
        MatematikaCanggih math= new MatematikaCanggih();
        int hasilPertambahan = math.Pertambahan(15, 5);
        System.out.println("hasil Pertambahan:" + hasilPertambahan);
        int hasilPerkalian = math.Perkalian(15,5);
        System.out.println("hasil Perkalian:" + hasilPerkalian);
        int hasilModulus = math.Modulus(17, 5);
        System.out.println("hasil Modulus:" + hasilModulus);
       
        
        
            
        }
        
    }
    

